[Appearance]
BoldIntense=true
ColorScheme=base16-tomorrow
Font=Liberation Mono,9,-1,5,50,0,0,0,0,0,Regular
LineSpacing=2
UseFontLineChararacters=true

[General]
Command=~/Util/tmux-tmp
LocalTabTitleFormat=%w
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=%w
TerminalColumns=90
TerminalRows=40

[Scrolling]
